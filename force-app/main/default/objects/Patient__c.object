    <validationRules>
        <fullName>Age_must_be_greater_than_4</fullName>
        <active>true</active>
        <errorConditionFormula>Patients_Age__c &lt; 4</errorConditionFormula>
        <errorMessage>Patients age must be greater than 4 years of Age. Contact Pediatric hospitalists if Patients age is below 4.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Appointment_Date_Greater_or_Equal</fullName>
        <active>true</active>
        <errorConditionFormula>Select_Appointment_Date__c &lt; TODAY()</errorConditionFormula>
        <errorMessage>How can one create Records for past ? Make sure Appointment Date is properly selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_Number_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>LEN(Contact_Number__c) &lt;= 6</errorConditionFormula>
        <errorMessage>Enter Correct Phone Number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>First_Name_Last_Name</fullName>
        <active>true</active>
        <errorConditionFormula>LEN(Name) &lt;= 2  ||  LEN(Patient_Last_Name__c) &lt;=2</errorConditionFormula>
